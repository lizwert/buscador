;################################### Definicion de TDA Index ###################################;


;***Representacion***:

;Un indice invertido sera representado por un conjunto de listas de la siguiente manera:
;
;  (define Index_Cualquiera '( lista_de_terminos )
;
;   Donde el indice invertido es un una lista que en su interior se compone por mas listas:
;
;              *lista_de_terminos : es una lista que se compone por una palabra y otra lista de documentos:
;
;                   (define lista_de_terminos_cualquiera '(palabra lista_documentos)) 
;
;                   *palabra: palabra asociada a una frecuencia y documentos 
;
;                   *lista_documentos: se compone de dos datos numericos : ID del documento y frecuencia.
;
;                         (define lista_documentos_cualquiera '(ID frecuency))
;
;              *cantidad_de:documentos: es un valor numerico que solo informa cuantos documentos existen en el indice.
;
;              *lista_de_datos_de_documentos: es una lista que se compone de los principales datos de un documento:
;
;              (define lista_de_datos_de_documentos_cualquiera '(ID_del_documento titulo autor bibliografia))
;
;                    *ID_del_documento: dato numero que indica que documento es.
;                    *titulo: es un arreglo de strings que corresponde al titulo del documento.
;                    *autor: es un arreglo de string que corrresponde a los autores del documento.
;                    *bibliografia: es un arreglo de string que corresponde a la bibliografia del documento.


(define (createIndex documents stopWords)
  (let ([termList (cleanStopWords stopWords (documentCleaner documents)) ])
    (if (null? termList)
        '()
        (createFullTermList termList documents)
    )
  )
)

(define (termQuery index term documents)
  (let ([termList (getTerm index term)])
    (createTermResult termList documents)
  )
)


(define (termSort list)
  (if (null? list)
      '()
      (cons (searchHigher list) (termSort (deleteNumber (searchHigher list) list)))
  )
)

(define (searchHigher list)
  (if (null? list)
      0
      (if (> (car list) (searchHigher (cdr list)))
          (car list)
          (searchHigher (cdr list))
      )
   )
)
          


(define (createTermResult Terms documents)
  (if (null? Terms)
        '()
        (if (list? (car Terms))
            (cons (cons (getFrecuency Terms (caar Terms))
                        (getDocument documents (caar Terms))
                  )
                  (createTermResult (cdr Terms) documents)
            )
            (createTermResult (cdr Terms) documents)
        )
  )
)
            
                  
                  
                  
                  
        
      
  
(define (getTerm listTerm term)
  (if (null? listTerm)
      '()
      (if (string=? (caar listTerm) term)
          (car listTerm)
          (getTerm (cdr listTerm) term)
      )
  )
)

(define (getFrecuency TermList documentID)
  (if (null? TermList)
      0
      (if (list? (car TermList))
          (if (= (caar TermList) documentID)
              (car (cdr (car TermList)))
              (getFrecuency (cdr TermList) documentID)
          )
          (getFrecuency (cdr TermList) documentID)
     )
  )
)
              
  
(define (getFrecuencyDocument word document)
  (let ([TermList (createListTerm document '())])
    (if (= (duplyCount TermList word) 0)
        '()
        (list (car document) (duplyCount TermList word))
    )
  )
)

(define (AllFrecuencyWord word documents)
  (if (null? documents)
      '()
      (if (null? (getFrecuencyDocument word (car(cdr (car documents)))))
          (AllFrecuencyWord word (cdr documents))
          (cons (getFrecuencyDocument word (car(cdr (car documents))))
            (AllFrecuencyWord word (cdr documents))
          )
      )
      
  )
)


(define (createFullTermList terms documents)
  (if (null? terms)
      '()
      (cons (cons (car terms) (AllFrecuencyWord (car terms) documents))
            (createFullTermList (cdr terms) documents)
      )
  )
)

        
(define (duplyCount list element)
  (if (null? list)
      0
      (if (equal? (car list) element)
          (+ (duplyCount (cdr list) element) 1)
          (duplyCount (cdr list) element)
      )
  )
)


(define (searchStringInList list string)
  (if (null? list)
      #f
      (if (string=? (car list) string)
          #t
          (searchStringInList (cdr list) string)
      )
  )
)



          
         
          


(define (createListTerm list emptyList)
  (if (null? list)
      emptyList
      (if (string? (car list))
          (createListTerm (cdr list) (append (separator (string->list (car list)) '()) emptyList))
          (createListTerm (cdr list) emptyList)
      )
  )
)

(define (removeID document)
  (cdr document)
)
      

(define (getAllTermList documents)
  (if (null? documents)
      '()
      (createListTerm
       (append
        (removeID(car (cdr (car documents))))
        (getAllTermList (cdr documents)))
     '()
     )
  )
)
              
          


          

(define (duply Lista1) 
  (cond 
    ((null? Lista1) Lista1) 
    ((searchStringInList (cdr Lista1) (car Lista1)) (duply (cdr Lista1)))
    (else (append (list (car Lista1)) (duply (cdr Lista1))))
  )
)



(define (cleanStopWords listStopWord lisTerm)
  (if (null? listStopWord)
      lisTerm
      (cleanStopWords (cdr listStopWord) (deleteItem  (car listStopWord) lisTerm))
  )
)





(define getDocument
  (lambda (list document)
    (if (null? list)
        '()
        (if (= (caar(cdr (car list))) document)
            (car(cdr(car list)))
            (getDocument (cdr list) document)
        )
    )
   )
)




(define (deleteItem x Lista1)
  (if (null? Lista1)
      '() 
      (if (string=? x (car Lista1)) 
          (deleteItem x (cdr Lista1)) 
          (cons (car Lista1) (deleteItem x (cdr Lista1)))
      )
  )
)

(define (deleteNumber x Lista1)
  (if (null? Lista1)
      '() 
      (if (= x (car Lista1)) 
          (deleteNumber x (cdr Lista1)) 
          (cons (car Lista1) (deleteNumber x (cdr Lista1)))
      )
  )
)



